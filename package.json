{
  "name": "react-router",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clean:build && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "clean:build": "cleandir build",
    "server": "lite-server -c internals/bs-config.json",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start:prod": "yarn run build && yarn run server",
    "postinstall": "node ./scripts/semantic.fix.js"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ],
      "react"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "preact-router": "2.5.2",
    "react-router-dom": "4.1.1",
    "styled-components": "1.4.6"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bundle-loader": "0.5.5",
    "clean-dir": "1.0.3",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "env-cmd": "5.1.0",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-clean-css": "1.5.1",
    "lite-server": "2.3.0",
    "postcss-loader": "2.0.5",
    "semantic-ui-less": "2.2.10",
    "source-map-loader": "^0.2.1",
    "standard": "10.0.2",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "standard": {
    "plugins": [
      "react"
    ]
  }
}
